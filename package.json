{
    "name": "app-bundler",
    "version": "0.0.89",
    "description": "",
    "main": "lib/bundler.js",
    "bin": {
        "app-bundler": "index.js"
    },
    "scripts": {
        "test": "atma test",
        "watch": "atma run tools/build --watch",
        "build": "atma run tools/build && atma run tools/build --cli",
        "server": "atma server",
        "release": "echo \"Run atma pub -m \"message\"\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/atmajs/app-bundler.git"
    },
    "keywords": [
        "module",
        "commonjs",
        "includejs",
        "bundle",
        "build",
        "package",
        "compiler"
    ],
    "author": "Alex Kit",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/atmajs/app-bundler/issues"
    },
    "homepage": "https://github.com/atmajs/app-bundler",
    "dependencies": {
        "alot": "^0.2.81",
        "appcfg": "^0.2.42",
        "atma-class": "^1.1.83",
        "atma-io": "^1.2.16",
        "atma-loader-ts": "^1.1.12",
        "atma-logger": "^1.0.2",
        "atma-utils": "^0.1.13",
        "cheerio": "^0.20.0",
        "includejs": "^0.15.44",
        "maskjs": "^0.64.41",
        "uglify-es": "^3.3.0"
    },
    "atma": {
        "plugins": [
            "atma-loader-ts"
        ],
        "settings": {
            "include": {
                "amd": true,
                "extentionDefault": {
                    "js": "ts"
                }
            },
            "io": {
                "extensions": {
                    "js": [
                        "atma-io-middleware-importer:read"
                    ],
                    "ts": [
                        "atma-io-middleware-importer:read",
                        "atma-loader-ts:read"
                    ]
                }
            },
            "atma-loader-ts": {
                "typescript": {
                    "compilerOptions": {
                        "noEmitHelpers": true,
                        "importHelpers": true,
                        "#if (TEST)": {
                            "module": "AMD"
                        },
                        "#if (!TEST)": {
                            "module": "CommonJS"
                        }
                    }
                }
            }
        }
    },
    "devDependencies": {
        "@types/node": "^8.5.2",
        "atma": "^0.12.7",
        "atma-io-middleware-importer": "^1.1.34",
        "selenium-query": "^0.50.35",
        "tslib": "^1.11.1"
    }
}
